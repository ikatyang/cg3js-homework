				skirtR.rotation.z = Math.PI / 6;
				skirtR.position.x = 12.5;
				skirt.add(skirtR);
				
				return danboard;
			}

			function createJoint() {
				return new THREE.Mesh(
					new THREE.SphereGeometry(5),
					new THREE.MeshLambertMaterial({ wireframe: true })
				);
			}

			function onWindowResize() {
				camera.aspect = window.innerWidth / window.innerHeight;
				camera.updateProjectionMatrix();
				renderer.setSize(window.innerWidth, window.innerHeight);
			}

			function animate() {
				animate.tick = animate.tick || 0;
				controls.update();
				if (gcontrols.animate) {
					var gui = guis[gcontrols.action];
					var delta = clock.getDelta();
					var isNewTick = (animate.tick % gui.animateOptions.tickLength + delta * gcontrols.speed > gui.animateOptions.tickLength);
					animate.tick += delta * gcontrols.speed;
					gui.updateTick(animate.tick % gui.animateOptions.tickLength);
					if (isNewTick)	{
						var audio = document.getElementById('dance-audio');
						audio.currentTime = animate.tick % gui.animateOptions.tickLength;
					}
					if (gcontrols.action === 'Run') {
						updateActionRun(delta);
					}
				}
				updateCamera();
				requestAnimationFrame(animate);
				renderer.render(scene, camera);
			}

			function onChangeAnimate(newValue) {
				var audio = document.getElementById('dance-audio');
				if (newValue) {
					clock.start();
					audio.play();
				} else {
					clock.stop();
					audio.pause();
				}
			}

			function onChangeAction(newValue) {
				for (var name in guis) {
					guis[name].domElement.style.display = 'none';
				}
				guis[newValue].domElement.style.display = '';
				animate.tick = 0;
				var audio = document.getElementById('dance-audio');
				danboard.position.set(0, 0, 0);
				danboard.rotation.set(0, 0, 0);
				guis[gcontrols.action].updateTick(0);
				if (newValue === 'Dance') {
					audio.muted = false;
					audio.currentTime = 0;
				} else {
					audio.muted = true;
				}
			}

			function onChangeSpeed(newValue) {
				var audio = document.getElementById('dance-audio');
				audio.playbackRate = newValue;
			}

			function updateActionRun(delta) {
				var self = arguments.callee;
				var radius = 50;
				var distance = 2 * ((60 * Math.PI) / 6 + 20) * gcontrols.speed * delta;
				self.angle = ((self.angle || 0)	+ (distance / radius)) % (Math.PI * 2);
				danboard.position.x = Math.cos(self.angle) * radius;
				danboard.position.z = Math.sin(self.angle) * radius;
				danboard.rotation.y = -self.angle;
			}

			function updateCamera() {
				if (gcontrols.perspective === '3rd') {
					var cameraPosition = new THREE.Vector3(0, 160, -100);
					camera.position.copy(danboard.localToWorld(cameraPosition));
					camera.lookAt(danboard.localToWorld(new THREE.Vector3(0, 0, 0)))
				}
			}

			function onChangePerspective(newValue) {
				if (newValue === 'god') {
					camera.position.set(0, 100, 300);
				}
			}

