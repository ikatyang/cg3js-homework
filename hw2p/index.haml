				var headMaterialArray = [
					new THREE.MeshLambertMaterial({
						side: THREE.DoubleSide,
						map: THREE.ImageUtils.loadTexture('../src/danboard/head1.png')
					}),
					new THREE.MeshLambertMaterial({
						side: THREE.DoubleSide,
						map: THREE.ImageUtils.loadTexture('../src/danboard/head2.png')
					}),
					new THREE.MeshLambertMaterial({
						side: THREE.DoubleSide,
						map: THREE.ImageUtils.loadTexture('../src/danboard/head3.png')
					}),
					new THREE.MeshLambertMaterial({ visible: false }),
					new THREE.MeshLambertMaterial({
						side: THREE.DoubleSide,
						map: THREE.ImageUtils.loadTexture('../src/danboard/head5.png')
					}),
					new THREE.MeshLambertMaterial({
						side: THREE.DoubleSide,
						map: THREE.ImageUtils.loadTexture('../src/danboard/head6.png')
					}),
				];
				var headMaterial = new THREE.MeshFaceMaterial(headMaterialArray);
				
				var bodyMaterialArray = [
					basicMaterial,
					basicMaterial,
					basicMaterial,
					new THREE.MeshLambertMaterial({ visible:false }),
					new THREE.MeshLambertMaterial({
						side: THREE.DoubleSide,
						map: THREE.ImageUtils.loadTexture('../src/danboard/body5.png')
					}),
